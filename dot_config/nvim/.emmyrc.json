{
  "$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
  "diagnostics": {
    "disable": [
      "param-type-not-match",
      "undefined-field",
      "undefined-global",
      "undefined-global"
    ]
  },
  "runtime": {
    "requirePattern": [
      "lua/?.lua",
      "lua/?/init.lua",
      "?/lua/?.lua",
      "?/lua/?/init.lua"
    ],
    "version": "LuaJIT"
  },
  "workspace": {
    "checkThirdParty": false
  }
}
