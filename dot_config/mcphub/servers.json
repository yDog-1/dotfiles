{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {
        "API_KEY": "",
        "SERVER_URL": null,
        "DEBUG": "true"
      }
    },
    "git": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "mcp-server-git"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": null
      }
    },
    "WXT": {
      "command": "npx",
      "args": [
        "-y",
        "sitemcp",
        "https://wxt.dev/",
        "-m",
        "/guide/**"
      ]
    }
  }
}
